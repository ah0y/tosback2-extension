{
    "name": "TOSback Extension",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "popup.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/cleaner_pepe-19.png",
        "default_title": "TOSback Tracker"
    },
    "permissions": [
        "https://a6700be0.ngrok.io/*",
        "tabs"
    ],
    "content_security_policy": 
        "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/prefixfree/1.0.7/prefixfree.min.js https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js; object-src 'self'"
    ,
    "icons": {
        "19": "images/cleaner_pepe-19.png",
        "48": "images/cleaner_pepe-48.png",
        "128": "images/cleaner_pepe-128.png"
    }
}